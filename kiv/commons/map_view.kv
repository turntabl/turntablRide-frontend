#:import GpsBlinker app.commons.gps_blinker
<MapView>:
    pos_hint: {"top": 1, "left": 1}
    size_hint: 1, 1
    id: mapview
    lat: 5.614818
    lon: -0.205874
    zoom: 12
    double_tap_zoom: True
    on_zoom:
        self.zoom = 12 if self.zoom < 12 else self.zoom
    on_touch_down: print("hello map")
    GpsBlinker:
        lat: root.lat
        lon: root.lon
        id: blinker
